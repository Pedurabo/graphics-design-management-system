{
  "name": "graphics-app-frontend",
  "version": "1.0.0",
  "description": "Frontend Team - Enhanced Graphics Application",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "team:standup": "echo 'Frontend Team Standup - 9:00 AM'",
    "team:sync": "echo 'Frontend Team Sync - 5:00 PM'",
    "team:retro": "echo 'Frontend Team Retrospective'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "typescript": "^5.0.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "three": "^0.150.0",
    "@react-three/fiber": "^8.11.0",
    "@react-three/drei": "^9.65.0",
    "fabric": "^5.2.1",
    "konva": "^8.3.0",
    "react-konva": "^18.2.0",
    "socket.io-client": "^4.6.1",
    "axios": "^1.3.4",
    "zustand": "^4.3.6",
    "react-query": "^3.39.3",
    "react-hook-form": "^7.43.5",
    "react-hot-toast": "^2.4.0",
    "tailwindcss": "^3.2.7",
    "framer-motion": "^10.0.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-color": "^2.19.3",
    "react-dropzone": "^14.2.3",
    "react-image-crop": "^10.1.8",
    "react-image-magnify": "^3.0.0",
    "react-image-zoom": "^1.0.0",
    "react-simple-maps": "^3.0.0",
    "react-svg": "^16.1.32",
    "react-spring": "^9.7.3",
    "react-transition-group": "^4.4.5",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.4.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "playwright": "^1.29.2",
    "storybook": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "prettier": "^2.8.4",
    "lighthouse": "^10.1.0",
    "vite": "^4.1.0",
    "vitest": "^0.28.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "team": {
    "name": "Frontend Team",
    "lead": "Frontend Architect",
    "standup": "9:00 AM",
    "sync": "5:00 PM",
    "sprint": "2 weeks",
    "focus": "React, TypeScript, Canvas API, WebGL"
  }
} 