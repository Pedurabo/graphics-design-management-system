{
  "name": "graphics-app-frontend-cluster",
  "version": "1.0.0",
  "description": "Frontend Cluster - Photoshop-level UI/UX with React and Canvas",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "performance": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "bundle-analyze": "npm run build && npx webpack-bundle-analyzer dist/stats.json",
    "team:standup": "echo 'Frontend Cluster Standup - UI/UX Focus'",
    "team:review": "echo 'Frontend Code Review Session'",
    "team:deploy": "echo 'Frontend Cluster Deployment'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "zustand": "^4.3.0",
    "react-query": "^3.39.0",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "framer-motion": "^10.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-color": "^2.19.0",
    "react-dropzone": "^14.2.0",
    "react-image-crop": "^10.0.0",
    "react-image-magnify": "^3.0.0",
    "react-image-zoom": "^1.0.0",
    "react-simple-maps": "^3.0.0",
    "react-svg": "^16.1.0",
    "react-spring": "^9.7.0",
    "react-transition-group": "^4.4.0",
    "styled-components": "^5.3.0",
    "tailwindcss": "^3.2.0",
    "three": "^0.150.0",
    "fabric": "^5.2.0",
    "konva": "^8.3.0",
    "socket.io-client": "^4.6.0",
    "canvas": "^2.11.0",
    "webgl": "^1.0.0",
    "webgl2": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/three": "^0.150.0",
    "@types/fabric": "^5.2.0",
    "@types/konva": "^8.3.0",
    "@types/canvas": "^2.11.0",
    "typescript": "^4.9.0",
    "vite": "^4.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "jest": "^29.0.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/jest-dom": "^5.16.0",
    "playwright": "^1.28.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "prettier": "^2.8.0",
    "storybook": "^6.5.0",
    "@storybook/react": "^6.5.0",
    "lighthouse": "^10.0.0",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "team": {
    "name": "Frontend Cluster",
    "lead": "UI/UX Specialist",
    "focus": "Photoshop-level Interface, Canvas Rendering, User Experience",
    "sprint_duration": "2 weeks",
    "daily_standup": "09:00 AM",
    "responsibilities": [
      "Photoshop-like UI/UX implementation",
      "Canvas-based graphics rendering",
      "Real-time collaboration features",
      "Performance optimization",
      "Cross-browser compatibility"
    ]
  }
} 